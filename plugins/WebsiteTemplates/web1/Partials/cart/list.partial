                                          <td>Shipping Charge : $<b class="js-product_total_shipping_charge">#data.total_shipping_charges#</b></td>
                                          <td>
                                             Tax : $#data.tax#
                                          </td>
                                          <td colspan="2">
                                             Sub Total :<span>$#data.subtotal#</span>
                                          </td>
                                       </tr>
                                    </tbody>
                                 </table>
